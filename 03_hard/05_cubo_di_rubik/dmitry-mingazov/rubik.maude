	op isOnlyWhiteCrossState : State -> Bool .
	ceq isOnlyWhiteCrossState(S) = true if isWhiteCrossState(S) and not(isOnlyWhiteCrossAlignedState(S)) .
	eq isOnlyWhiteCrossState(S) = false [owise] .

