	op yCross : State -> State .
	eq yCross(S) = Ri(U(U(R(U(Ri(U(R(S)))))))) .

	op yCrossi : State -> State .
	eq yCrossi(S) = L(Ui(Ui(Li(Ui(L(Ui(Li(S)))))))) .

